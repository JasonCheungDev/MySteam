    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.communityvisibilitystate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.profilestate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.personaname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.lastlogoff)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.profileurl)

                <th>
                    @Html.DisplayNameFor(model => model.personastateflags)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.loccountrycode)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.locstatecode)

@model IEnumerable<MySteam.Models.UserModel>

@{
    ViewData["Title"] = "UserModel";
}

<h2>UserModel</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.communityvisibilitystate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.profilestate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.personaname)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.lastlogoff)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.profileurl)

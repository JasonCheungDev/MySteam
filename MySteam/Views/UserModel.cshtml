                @Html.DisplayFor(modelItem => item.avatarmedium)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.avatarfull)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.personastate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.realname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.primaryclanid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.timecreated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.personastateflags)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.loccountrycode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.locstatecode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.loccityid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.steamid">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.steamid">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.steamid">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

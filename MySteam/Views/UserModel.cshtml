                <th>
                    @Html.DisplayNameFor(model => model.avatarmedium)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.avatarfull)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.personastate)
